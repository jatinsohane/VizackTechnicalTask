    <body>



<div class="page-login">
  <div class="ui centered grid container">
    <div class="nine wide column">
       
      <div class="ui icon warning message">
          <i class="lock icon"></i>
          <div class="content">
            <div class="header">
              Login 
            </div>
            <p>Make your way in</p>
          </div>
        </div>
      <div class="ui fluid card">
        <div class="content">
        <form class="ui form" method="POST" action="/login">
          <div class="field">
            <label>Username</label>
            <input type="text" name="username" placeholder="Vizag Team">
          </div>
          <div class="field">
            <label>Password</label>
            <input type="password" name="password" placeholder="Password">
          </div>
          <input class="ui primary submit labeled icon button" type="submit">
            <!--<i class="unlock alternate icon"></i>      <input class="ui submit button" type="submit">-->
            <!--Login-->
          <!--</button>-->
        </form>
        <p></p>
        <p><a href="/register">New user? Sign up</a></p>
        </div>
      </div>
    </div>
  </div>
</div>



</body>
